{
    "manifest_version": 3,
    "name": "ScribdHack",
    "description": "Blur remover for SCRIBD",
    "version": "1.0",
    "content_scripts": [
        {
            "js": [
                "enable-selection.js"
            ],
            "css": [
                "remove-blur.css",
                "remove-promo.css",
                "enable-selection.css"
            ],
            "matches": [
                "https://www.scribd.com/document/*/*"
            ]
        }
    ]
}